{
  "name": "devise-ante",
  "version": "0.0.0",
  "description": "Frontend for Devise",
  "main": "index.js",
  "scripts": {
    "cm": "git-cz",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test:lint:js": "standard --fix",
    "test:lint:scss": "stylelint '**/*.vue' --syntax scss",
    "test:format": "prettier-standard src/**/*.js",
    "test": "run-s test:**",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "linters": {
      "**/*.js": [
        "standard --fix",
        "prettier-standard",
        "git add"
      ]
    }
  },
  "repository": "git@github.com:multi-cell/devise-ante",
  "author": "Graham Barber <grahammbarber@gmail.com>",
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "at-ui": "^1.2.0-beta",
    "at-ui-style": "^1.3.2",
    "vue": "^2.3.4",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "commitizen": "^2.9.6",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^3.13.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-loader": "^2.0.6",
    "prettier-standard": "^7.0.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "standard": "^10.0.3",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-processor-html": "^1.0.0",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.0.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.4",
    "webpack": "^3.0.0",
    "webpack-dashboard": "^1.0.0-5",
    "webpack-dev-server": "^2.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
